.warpper {
    width: 100%;
    height: 100%;
    overflow: hidden;
    text-align: center;
}

.image {
    width: 300rpx;
    height: 300rpx;
    margin-top: 146rpx;
}

.tip-title {
    margin-top: 74rpx;
    margin-bottom: 12rpx;
    font-size: 40rpx;
    color: #353535;
}

.tip-desc {
    font-size: 32rpx;
    color: #979797;
}

.fileline {
    margin-top: 74rpx;
    margin: 74rpx 40rpx 0 40rpx;
    text-align: center;
}

.filename {
    max-width: 100%;
    display: inline-block;
    color: #2E68F8;
    font-size: 40rpx;
    vertical-align: middle;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.rename {
    display: inline-block;
    height: 36rpx;
    width: 36rpx;
    margin-left: 8rpx;
    vertical-align: middle;
}

.btn {
    margin: 0 40rpx;
    background: #2E68F8;
    border-radius: 8rpx;
    font-size: 36rpx;
    color: #FFFFFF;
    text-align: center;
}

.btnHover {
    background-color: #295DDE;
}

.returnBtn {
    background: #fafafa;
    border: 3rpx solid #E2E2E2;
    border-radius: 8rpx;
    font-size: 36rpx;
    color: #353535;
    text-align: center;
}

.returnBtnHover {
    background-color: #F0F0F0;
}

.tip {
    font-size: 28rpx;
    color: rgba(151,151,151,1);
    margin-top: 28rpx;
}

.bottom {
    margin-top: 80rpx;
}