page {
    padding-bottom: 160rpx;
}

.clz-top-menu-item {
    padding: 0;
    width: 100%;
    height: 120rpx;
    display: flex;
    background: rgba(255,255,255,1);
    flex-direction: row;
    align-items: center;
}

.clz-top-menu-item-title {
    padding-left: 36rpx;
    font-size: 32rpx;
    color: #353535;
    flex: 1;
    white-space: nowrap;
    text-overflow: ellipsis;
    text-align: left;
    overflow: hidden;
    word-wrap: normal;
}

.btnHover {
    background: transparent;
}

.arrow {
    width: 36rpx;
    height: 36rpx;
    margin-right: 36rpx;
}

.clz_list_head_seperator {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    background: #F5F5F5;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(155,155,155,1);
}

.clz_list_head_title {
    height: 120rpx;
    line-height: 120rpx;
    background: white;
    padding-left: 36rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151,151,151,1);
}

.selected_contacst_avatars_list_container_place_holder {
    width: 100%;
    height: 120rpx;
    position: relative;
}

.selected_contacts_avatars_list_container {
    width: 100%;
    height: 120rpx;
    position: relative;
}

.position_fixed {
    position: fixed;
    top: 0;
    width: 100%;
    background: white;
    z-index: 100;
}

.selected_contacts_avatars_list_scroll-view {
    position: absolute;
    left: 36rpx;
    right: 200rpx;
    height: 120rpx;
}

.selected_contacts_avatars_list {
    position: relative;
    height: 120rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.selected_avatar {
    width: 60rpx;
    height: 60rpx;
    flex-shrink: 0;
    flex-grow: 0;
    border-radius: 50%;
}

.selected_avatar_tip {
    flex: none;
    position: absolute;
    top: 39rpx;
    right: 36rpx;
    width: 160rpx;
    height: 42rpx;
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(178,178,178,1);
    line-height: 42rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: right;
    white-space: nowrap;
}

.selected_contacts_avatar_container_bottomline {
    flex: none;
    width: 100%;
    height: 1rpx;
    background: rgba(229,229,229,1);
    position: absolute;
    bottom: 0;
}

.contact-item {
    height: 120rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.contact-item-avatar {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin: 0 50rpx 0 36rpx;
    background-color: gainsboro;
}

.contact-item-name {
    font-size: 32rpx;
    color: #353535;
    flex: 2;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-wrap: normal;
}

.contact-item-line {
    margin-left: 36rpx;
    height: 1rpx;
    background-color: #f5f5f5;
}

checkbox .wx-checkbox-input {
    border-radius: 50%;
    border-width: 1rpx;
    width: 38rpx;
    height: 38rpx;
    margin: 0 36rpx 0 20rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    border-radius: 50%;
    border-width: 1rpx;
    background: #2e68f8;
    border-color: #2e68f8;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    border-radius: 50%;
    border-width: 1rpx;
    width: 38rpx;
    height: 38rpx;
    line-height: 38rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.checkbox-disabled .wx-checkbox-input.wx-checkbox-input-checked {
    border-radius: 50%;
    border-width: 1rpx;
    background: rgb(171,195,252);
    border-color: rgb(171,195,252);
}

.checkbox-disabled .wx-checkbox-input.wx-checkbox-input-checked::before {
    border-radius: 50%;
    border-width: 1rpx;
    width: 38rpx;
    height: 38rpx;
    line-height: 38rpx;
    text-align: center;
    font-size: 26rpx;
    color: rgba(255,255,255,0.40);
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.bottom-align {
    width: 100%;
    background-color: #fff;
    position: fixed;
    top: 100%;
    left: 100%;
    transform: translate(-100%,-100%);
}

.clz-btn-add-member {
    margin: 30rpx 40rpx;
    width: 670rpx;
    height: 92rpx;
    line-height: 92rpx;
    color: #fff;
    background-color: #2e68f8;
    border-radius: 8rpx;
}

.clz-btn-add-member[disabled]:not([type]) {
    color: #FFFFFF;
    background-color: rgba(46,104,248,0.40);
}

.top_small_avatars_container {
    flex: none;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    overflow: hidden;
    margin: 200rpx auto 0rpx;
    background: #E7E9EB;
    display: flex;
    flex-wrap: wrap-reverse;
    justify-content: center;
    align-items: center;
}

.small_avatar {
    width: 60rpx;
    height: 60rpx;
}

.top_icon {
    margin: 200rpx auto 0rpx;
    width: 120rpx;
    height: 120rpx;
    display: block;
}

.top_title {
    margin-top: 45rpx;
    margin-left: 90rpx;
    margin-right: 90rpx;
    text-align: center;
    font-size: 32rpx;
    color: #353535;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.avatar_container {
    margin-top: 60rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.avatar_center {
    display: flex;
    flex-direction: row-reverse;
}

.avatar {
    width: 68rpx;
    height: 68rpx;
    border: 5rpx solid #fff;
    border-radius: 50%;
}

.avatar_right_text {
    font-size: 30rpx;
    color: #B2B2B2;
    margin-left: 25rpx;
}

.bottom_container {
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;
}

.bottom_button {
    margin-left: 40rpx;
    margin-right: 40rpx;
    height: 92rpx;
    border-radius: 8rpx;
    margin-bottom: 30rpx;
}

.blue_button {
    background: #2E68F8;
    color: #FFF;
}

.grey_button {
    color: #353535;
    background: #F0F0F0;
    border: 3rpx solid #E2E2E2;
}

.join_button {
    margin-bottom: 118rpx;
}

.retry_button {
    margin-top: 120rpx;
    margin-bottom: 0rpx;
}

.bottom_text {
    color: #979797;
    text-align: center;
    display: block;
}

.invite_prompt {
    font-size: 28rpx;
    margin-bottom: 48rpx;
}

.invite_role_prompt {
    font-size: 32rpx;
    margin-bottom: 240rpx;
}

.join_prompt {
    font-size: 32rpx;
    margin-bottom: 30rpx;
}

.back_main_text_container {
    flex: auto;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
}

.back_main_text {
    height: 136rpx;
    line-height: 136rpx;
    color: #2E68F8;
    font-size: 28rpx;
    text-align: center;
}

.error_container {
    position: absolute;
    top: 0rpx;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.error_image {
    width: 300rpx;
    height: 300rpx;
    margin-top: 180rpx;
}

.error_text {
    text-align: center;
}

.error_title {
    font-size: 40rpx;
    color: #353535;
    margin-top: 40rpx;
}

.error_desc {
    font-size: 32rpx;
    color: #979797;
    margin-top: 10rpx;
}

.back_main {
    margin-top: 30rpx;
    margin-bottom: 80rpx;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.clz_contacts_empty_tip {
    margin-top: 280rpx;
    background: white;
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151,151,151,1);
