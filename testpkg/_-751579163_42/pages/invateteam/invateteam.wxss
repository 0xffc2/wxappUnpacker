}

.clz-approve-committed-title {
    height: 54rpx;
    font-size: 40rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(53,53,53,1);
    line-height: 56rpx;
    margin-top: 76rpx;
    text-align: center;
}

.clz-approve-committed-sub-title {
    height: 148rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151,151,151,1);
    line-height: 45rpx;
    margin-top: 27rpx;
    text-align: center;
}